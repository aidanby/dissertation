{
  "latex-workshop.latex.tools": [
    {
      "name": "pdflatex",
      "command": "/usr/local/texlive/2025/bin/universal-darwin/pdflatex",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "%DOC%"
      ]
    },
    {
      "name": "bibtex",
      "command": "/usr/local/texlive/2025/bin/universal-darwin/bibtex",
      "args": [
        "%DOCFILE%"
      ]
    }
  ],
  "latex-workshop.latex.recipes": [
    {
      "name": "pdflatex -> bibtex -> pdflatex*2",
      "tools": [
        "pdflatex",
        "bibtex",
        "pdflatex",
        "pdflatex"
      ]
    }
  ],
  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.latex.clean.fileTypes": [
    "*.aux",
    "*.bbl",
    "*.blg",
    "*.idx",
    "*.ind",
    "*.lof",
    "*.lot",
    "*.out",
    "*.toc",
    "*.acn",
    "*.acr",
    "*.alg",
    "*.glg",
    "*.glo",
    "*.gls",
    "*.fls",
    "*.log",
    "*.fdb_latexmk",
    "*.snm",
    "*.synctex(busy)",
    "*.synctex.gz(busy)",
    "*.nav"
  ],
  "latex-workshop.texcount.path": "/usr/local/texlive/2025/bin/universal-darwin/texcount",
  "latex-workshop.texcount.args": [
    "-conf=.texcountrc",
    "%DOC%"
  ],
  "latex-workshop.texcount.autorun": "never"
}